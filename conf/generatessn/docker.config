timeline.overwrite = true
dag.overwrite = true
report.overwrite = true

docker.enabled = true

process {
    cpus = 1
    memory = 2.GB
    container = 'efi-est:latest'
    containerOptions = "-v $projectDir:$projectDir -v $projectDir/../../:$projectDir/../../"

    withName: filter_blast {
        memory = 16.GB
    }

    withName: create_full_ssn {
        memory = 16.GB
    }
}
