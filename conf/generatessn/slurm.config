timeline.overwrite = true
dag.overwrite = true
report.overwrite = true

executor {
    name = 'slurm'
    queue = 'efi'
    queueSize = 128
}

process {
    executor = 'local'
    cpus = 1
    memory = 16.GB

    withName: filter_blast {
        executor = 'slurm'
        queue = 'efi'
        cpus = 1
        memory = 50.GB
    }

    withName: create_xgmml_100 {
        executor = 'slurm'
        queue = 'efi'
        cpus = 1
        memory = 150.GB
    }
}