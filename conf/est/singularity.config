timeline.overwrite = true
dag.overwrite = true
report.overwrite = true

singularity.enabled = true

process {
    cpus = 1
    memory = 2.GB
    container = 'enzymefunctioninitiative/efi-est:latest'
    containerOptions = "--bind $projectDir:$projectDir --bind $projectDir/../../:$projectDir/../../"

    withName: blastreduce {
        cpus = 1
        memory = 16.GB
    }

    withName: visualize {
        cpus = 2
        memory = 8.GB
    }
}
